<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="5dp"
    android:backgroundTint="@color/button_background"
    android:backgroundTintMode="multiply"
    tools:context=".MainActivity">
    <Button
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Save to module"
        android:id="@+id/buttonGlobalSaveToModule"
        android:textAlignment="center"
        android:layout_margin="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonGlobalConsole"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Console"
        android:id="@+id/buttonGlobalConsole"
        android:textAlignment="center"
        android:layout_margin="5dp"
        app:layout_constraintStart_toEndOf="@+id/buttonGlobalSaveToModule"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <!--
        -->
<!--
</LinearLayout>
